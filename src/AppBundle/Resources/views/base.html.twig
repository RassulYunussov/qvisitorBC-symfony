<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8"/>
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
         <!-- Bootstrap Core CSS -->
	<link href="{{ asset('bootstrap1/dist/css/sb-admin-2.css')}}" rel="stylesheet">
	<link href="{{ asset('bootstrap1/dist/css/timeline.css')}}" rel="stylesheet" type="text/css">
	<link href="{{ asset('bootstrap1/dist/css/breadcrumbs.css')}}" rel="stylesheet" type="text/css">
	<link href="{{ asset('bootstrap1/bower_components/bootstrap/dist/css/bootstrap.min.css')}}" rel="stylesheet" type="text/css">
	<link href="{{ asset('bootstrap1/bower_components/metisMenu/dist/metisMenu.min.css')}}" rel="stylesheet" type="text/css">
	<link href="{{ asset('bootstrap1/bower_components/morrisjs/morris.css')}}" rel="stylesheet" type="text/css">
	<link href="{{ asset('bootstrap1/bower_components/font-awesome/css/font-awesome.min.css')}}" rel="stylesheet" type="text/css">
	
	
	{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
	    {% block head%} {% endblock%}
	<div id="wrapper">
		{% block navbar%}
			
			<nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
			
				<div class="navbar-header">
					<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
						<span class="sr-only">Toggle navigation</span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</button>
{% block header%}

{% endblock %}				
				</div>
				
				{{ render(controller('AppBundle:UserProfile:profileMenu')) }}
					
				<div class="navbar-default sidebar" role="navigation">
					<div class="sidebar-nav navbar-collapse">
						<ul class="nav" id="side-menu">
							{{ render(controller('AppBundle:PageMenu:menu')) }}
						</ul>
					</div>
				</div>
			</nav>
		{%endblock%}
			
			{% block body %}
				{% block breadcrumb %}{% endblock %}
			{% endblock %}
			{% block foot%} {% endblock %}
	</div>
	
	
        {% block javascripts %}
		

		<script src="{{asset('bootstrap1/bower_components/jquery/dist/jquery.min.js')}}"></script>
		<script src="{{asset('bootstrap1/bower_components/bootstrap/dist/js/bootstrap.min.js')}}"></script>
		<script src="{{asset('bootstrap1/bower_components/metisMenu/dist/metisMenu.min.js')}}"></script>
		<script src="{{asset('bootstrap1/bower_components/raphael/raphael-min.js')}}"></script>
		<script src="{{asset('bootstrap1/bower_components/morrisjs/morris.min.js')}}"></script>
		<script src="{{asset('bootstrap1/dist/js/sb-admin-2.js')}}"></script>
        {% endblock %}
    </body>
</html>